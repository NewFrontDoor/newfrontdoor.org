{
  "name": "gulp-tasks",
  "version": "1.0.0",
  "description": "A collection of gulp tasks I use on different projects",
  "main": "gulpfile.js",
  "scripts": {
    "build": "gulp build --set-env-NODE_ENV=production",
    "start": "gulp",
    "test": "xo && ava"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BarryThePenguin/gulp-tasks.git"
  },
  "author": "Jonathan Haines",
  "contributors": [
    {
      "name": "Jonathan Haines"
    },
    {
      "name": "Alan Reader"
    }
  ],
  "license": "MIT",
  "dependencies": {
    "lodash": "4.13.1",
    "lunr": "0.7.1",
    "moment": "2.13.0",
    "node-sass": "3.7.0"
  },
  "devDependencies": {
    "argv-set-env": "1.0.1",
    "autoprefixer": "6.3.6",
    "ava": "0.15.1",
    "babel": "6.5.2",
    "babel-core": "6.9.1",
    "babel-loader": "6.2.4",
    "babel-plugin-lodash": "3.1.4",
    "babel-polyfill": "6.9.1",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-2": "6.5.0",
    "bootstrap-sass": "3.3.6",
    "browser-sync": "2.12.10",
    "browser-sync-webpack-plugin": "1.0.1",
    "classnames": "2.2.5",
    "css-loader": "0.23.1",
    "del": "2.2.0",
    "eslint-config-xo-react": "0.7.0",
    "eslint-plugin-react": "5.1.1",
    "eval": "0.1.1",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "front-matter": "2.0.8",
    "gulp": "3.9.1",
    "gulp-filter": "4.0.0",
    "gulp-front-matter": "1.3.0",
    "gulp-load-plugins": "1.2.4",
    "gulp-notify": "2.2.0",
    "gulp-plumber": "1.1.0",
    "gulp-util": "3.0.7",
    "html-loader": "0.4.3",
    "html-webpack-plugin": "2.18.0",
    "indexhtml-webpack-plugin": "0.1.9",
    "json-loader": "0.5.4",
    "lodash-webpack-plugin": "0.9.1",
    "lost": "6.8.0",
    "ngrok": "2.1.8",
    "node-libs-browser": "1.0.0",
    "normalize.css": "4.1.1",
    "postcss-colorblind": "BarryThePenguin/postcss-colorblind#patch-1",
    "postcss-loader": "0.9.1",
    "postcss-pxtorem": "3.3.1",
    "postcss-vertical-rhythm": "1.1.5",
    "raw-loader": "0.5.1",
    "react": "15.1.0",
    "react-dom": "15.1.0",
    "react-router": "2.4.1",
    "react-scroll": "1.0.16",
    "react-sticky": "5.0.2",
    "react-waypoint": "2.0.2",
    "remark": "4.2.2",
    "remark-react": "2.1.0",
    "remark-toc": "3.0.0",
    "remove-markdown": "0.1.0",
    "sass-loader": "3.2.0",
    "slash": "1.0.0",
    "static-site-generator-webpack-plugin": "2.1.0",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "1.13.1",
    "webpack-dev-server": "1.14.1",
    "webpack-notifier": "1.3.0",
    "xo": "0.15.1",
    "xo-loader": "0.6.0"
  },
  "ava": {
    "files": [
      "**/*.test.js"
    ],
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "xo": {
    "esnext": true,
    "extends": "xo-react",
    "rules": {
      "react/no-danger": 1,
      "xo/filename-case": 0
    },
    "envs": [
      "browser"
    ]
  },
  "babel": {
    "plugins": [
      "lodash"
    ],
    "presets": [
      "react",
      "es2015",
      "stage-2"
    ]
  }
}
